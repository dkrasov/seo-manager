{
    "name": "dkrasov/seo-manager",
    "description": "SEO Manager for Laravel Framework",
    "keywords": ["laravel", "seo", "seo-manager", "lionix", "meta", "meta-data", "open-graph", "og-data"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmytro Krasov",
            "email": "dskrasov@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Krasov\\SeoManager\\SeoManagerServiceProvider"
            ],
            "aliases": {
                "SeoManager": "Krasov\\SeoManager\\Facades\\SeoManager"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Krasov\\SeoManager\\": "src"
        },
        "files":[
            "src/helpers/helpers.php"
        ]
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.0.*"
    }
}
